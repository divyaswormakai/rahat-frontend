{
  "name": "cash-aid",
  "version": "1.0.0",
  "description": "cash transfer process through token",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "cross-env ENV_TYPE=development nodemon ./bin/www",
    "client": "cd ./client && yarn dev",
    "setup": "yarn install && cd ./client && yarn install",
    "build": "cross-env ENV_TYPE=production yarn install && cd ./client && yarn install && yarn run prod"
  },
  "repository": {
    "type": "git",
    "url": "git@lab.rumsan.net:esatya/cash-aid.git"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "client/*",
      "play/*",
      "config/*"
    ]
  },
  "author": "Manjik",
  "license": "ISC",
  "dependencies": {
    "@truffle/hdwallet-provider": "^1.0.38",
    "axios": "^0.19.2",
    "bip39": "^3.0.2",
    "compression": "^1.7.4",
    "config": "^3.2.2",
    "cookie-parser": "~1.4.3",
    "cross-env": "^7.0.2",
    "crypto-js": "^3.1.9-1",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "eth-sig-util": "^2.5.3",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^7.0.2",
    "express": "~4.16.0",
    "fs": "^0.0.1-security",
    "handlebars": "^4.0.12",
    "hdkey": "^2.0.1",
    "http-errors": "~1.6.2",
    "lodash": "^4.17.15",
    "mongoose": "^5.2.9",
    "morgan": "~1.9.0",
    "multer": "^1.4.2",
    "nodemailer": "^4.7.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "rs-user": "git+ssh://git@lab.rumsan.net:rs/user.git",
    "rumsan-core": "^1.0.1",
    "sha256": "^0.2.0",
    "twilio": "^3.43.1",
    "web3": "^1.2.0",
    "xls-to-json-lc": "^0.3.4",
    "xlsx-to-json-lc": "^0.5.0"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
