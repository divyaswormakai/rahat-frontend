<div id="<%=id%>" class="modal fade" role="dialog">
  <div class="modal-dialog">
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">Upload Beneficiary Data</h4>
      </div>
      <div class="modal-body">
        <form id="frmUpload">
          <div class="row">
            <div class="col-md-12">
              <div class="form-group">
                <label><b>Aid Project:</b></label>
                <select id="aid_select_upload" value="" name="aidIdUpload" class="form-control"
                  data-validation="required"></select>
              </div>
            </div>
            <input type="file" name="file" id="uploadedFile" />
          </div>
          <div class="hr-line-dashed"></div>
          <div class="form-group row">
            <div class="col-sm-4 col-sm-offset-2">
              <button type="button" class="btn btn-sm btn-default" data-dismiss="modal">
                Close
              </button>
              <button class="btn btn-primary btn-sm" type="submit">
                Submit
              </button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>